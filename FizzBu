function buzz(x){
  
  for(let i=1;i<=100;i++){
    x+=1
    let schet=0
     if(x%3==0 && x%5==0){schet="FIZZBUZZ"
      console.log(schet)
    }
    else if(x%3==0){schet="Fizz"
      console.log (schet)
    }
    else if(x%5==0){schet="Buzz"
      console.log(schet)
    }
   
    else{console.log(x)}
  }
}

buzz(0)
